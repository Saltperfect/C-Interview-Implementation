class Selection-sort{
  public:
    Selection-sort(){}
    void sort(vector<int>& a, int size){
      for(i=0; i<size; i++){
          int min = i;
      	for(j=i+1; j<size; j++){
	  if(a[j] < a[min])
	    min = j;
	  swap(a[i], a[min]);
	}
      }
    }
};
int main(){
  Selection-sort s1;
  vector<int>k = {12,45,65,89,74,32,45};
  int s = 7;
  s1.sort(k,s);
}
